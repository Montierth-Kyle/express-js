<h1 style="text-align:center" class="main-title">Courses Offered</h1>
<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Description</th>
            <th>Length</th>
        </tr>
    </thead>
    <tbody>
        <% courses.forEach( function(course) { %>
            <tr>
                <td>
                    <a href=<%='/courses/' + course.id %> >Show</a>
                </td>
                <td>
                    <%= course.name%>
                </td>
                <td>
                    <%= course.base_price%>
                </td>
                <td>
                    <%= course.description%>
                </td>
                <td>
                    <%= course.length%>
                </td>
            </tr>

            <% }) %>
    </tbody>
</table>
